* {
  background-color: transparent; 
  border-color: #444;
  text-color: #bbb;
  spacing: 0;
  fullscreen: true;
}

window {
  margin: 50% 33.33% 11.11% 33.33%;
}

inputbar {
  border: 0 0 0 0;
  children: [prompt,entry];
}

prompt {
  padding: 16px;
  border: 0 0 0 0;
}

entry {
  padding: 16px;
}

listview {
  cycle: false;
  margin: 0 0 -1px 0;
  scrollbar: false;
}

element {
 padding: 16px;
 text-color: #bbb;
}

element selected {
  text-color: #aaa;
  background-color: #4445;
}
