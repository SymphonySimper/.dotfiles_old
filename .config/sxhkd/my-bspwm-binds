# My bspwm binds

# To launch coidng time
super + x
	rofi -show coding-time -modi "coding-time:$HOME/.local/bin/bspwm/coding-time"

# To resize left side window to approx 70%
super + shift + x
	bspc node -z right +400 0

# Play copied url
super + shift + m
	bspc desktop -f '^4'; \
	prime-run $VIDEOPLAYER "$(xclip -selection clipboard -o)"

