# Open Terminal
super + {Return,shift + Return}
 	{$TERMINAL,$SECOND_TERMINAL}

# Open Browser/ Lf
super + {f, shift + f}
	{$TERMINAL -e lfp,$BROWSER}

# Volume
super + {F2,F3,F4,shift + F2,shift + F3}
	volume -{d,u,m,D,U}
super + F12
	$TERMINAL -e pulsemixer

# Music
super + {F1,shift + F1}
	{play-bg,bmark open spotify}

# Get work done
super + shift + F12
	$TERMINAL -e gen-hosts

# Brightness
super + {F5,F6}
	brightness -{u,d}

# Picom
super + shift + p
	game-mode

# Rofi
super + {space,s,e,r,a,shift+n}
	rofi-{run,open,edit,todo,play,notes}
super + shift + q
	rofi -modi "Power Options:$HOME/.local/bin/rofi/power-options" \
	-show 'Power Options'

# Screenshot
super + shift + {s,a}
	screenshot -{r,w}
Print
	screenshot -s

# My info/ goals
super + {b,shift + b}
	{my-info,goals}

# Screenkey
XF86Calculator
	typin

# htop
super + F11
	$TERMINAL -e htop

# vimwiki
super + w
	$TERMINAL -e mywiki edit

# RSS
super + n
	$TERMINAL -e $RSS_READER

# dunst
super + shift + space
	dunstctl action

# Play copied url
super + shift + m
	prime-run $VIDEO_PLAYER "$(xclip -selection clipboard -o)"

# bmark
super + i
	bmark open figma

# sxhkd
super + Escape
	re-key
