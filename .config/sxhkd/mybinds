# Open Terminal
super + Return
 	$TERMINAL
super + shift + Return
	$SECONDTERMINAL	

# Open Browser
super + shift + f
	$BROWSER
super + shift + m
	ff2mpv

# Volume
XF86AudioLowerVolume
	volume -d
XF86AudioRaiseVolume
	volume -u
XF86AudioMute
	volume -m
super + F2
	pmixer

# Brightness
super + F5
	brightness -u
super + F6
	brightness -d

# Picom
super + shift + p
	gameMode

# Rofi
super + space
	rofi -show run	
super + s
	rofiOpen
super + e
	rofiEdit
super + r
	rofiTodo

# Screenshot
super + alt + s
	screenshot -r
super + alt + a
	screenshot -w
Print
	screenshot -s

# My info
super + b
	myInfo

# Screenkey
XF86Calculator
	typin

# sxhkd
super + Escape
	rekey
