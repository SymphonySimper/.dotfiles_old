# Open Terminal
super + Return
 	$TERMINAL
super + shift + Return
	$SECONDTERMINAL	

# Open Browser
super + shift + f
	$BROWSER

# Volume
super + F2
	volume -d
super + F3
	volume -u
super + shift + F2
	volume -D
super + shift + F3
	volume -U
super + F4
	volume -m
super + F12
	$TERMINAL -e pulsemixer

# Lofi
super + F1
	playLofi

# Get work done
super + shift + F12
	$TERMINAL -e genHosts

# Brightness
super + F5
	brightness -u
super + F6
	brightness -d

# Picom
super + shift + p
	gameMode

# Rofi
super + space
	rofi -show run	
super + s
	rofiOpen
super + e
	rofiEdit
super + r
	rofiTodo
super + a
	rofiPlay	

# Screenshot
super + alt + s
	screenshot -r
super + alt + a
	screenshot -w
Print
	screenshot -s

# My info
super + b
	myInfo

# Screenkey
XF86Calculator
	typin

# sxhkd
super + Escape
	rekey
