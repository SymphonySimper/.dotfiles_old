#Git alias
alias g='git'
alias gc='git clone'
alias gs='git status'
alias gp='git pull'
alias ga='git add'
alias gau='git add -u'
alias gnotrack='git config --local status.showUntrackedFiles no'

#Config git alias
alias cgs='config status'
alias cgp='config push'
alias cga='config add'
alias cgau='config add -u'
